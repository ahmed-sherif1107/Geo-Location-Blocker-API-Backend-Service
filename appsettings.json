{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "IPGeolocation": {
    "ApiKey": "Your_API_Key",
    "BaseUrl": "https://api.ipgeolocation.io/",
    "RateLimit": {
      "RequestsPerMinute": 30
    }
  },
  "Blocking": {
    "MinTemporaryBlockMinutes": 1,
    "MaxTemporaryBlockMinutes": 1440,
    "DefaultPageSize": 10,
    "MaxPageSize": 100
  }
}